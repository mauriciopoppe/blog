    "start:webpack": "webpack-dev-server --config webpack.config.dev.js --hot",
    "build": "NODE_ENV=production node src/util/sitemap-tree-generator.js && npm run generate:colors && npm run build:webpack && npm run build:hugo",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:hugo": "hugo -d ../dist -s site -v",
    "serve:prod": "http-server -S -C site/cert/localhost.pem -K site/cert/localhost-key.pem dist",
    "generate:colors": "node src/util/palette-generator.js && bulma-css-vars",
    "generate:sitemap": "node src/util/sitemap-tree-generator.js"
